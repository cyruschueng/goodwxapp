/* 列表内容 */
.myOrderBox {
  width: 100%;
  background-color: #ebebeb;
  font-size: 32rpx;
}

.personnelWrap {
  width: 100%;
  margin-bottom: 20rpx;
  background-color: #fff;
}

.personnelTime {
  width: 100%;
  height: 78rpx;
  line-height: 78rpx;
  display: flex;
  flex-direction: row;
  padding: 0 28rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #e0e0e0;
  font-size: 26rpx;
}

.personnelTime_left {
  width: 70%;
  color: #b1b1b1;
}

.personnelTime_right {
  width: 30%;
  color: #555;
  text-align: right;
}

.personnelTime_right text.active {
  color: #fc5e2d;
}

.personnelTime_minute {
  margin-left: 40rpx;
}

.personnelOrder {
  width: 100%;
  height: 212rpx;
  background-color: #fff;
  display: flex;
  flex-direction: row;
}

.iconLogoWrap {
  width: 25%;
  height: 212rpx;
  display: flex;
  justify-content: center;
}

.iconLogo_sj {
  width: 130rpx;
  height: 130rpx;
  padding-top: 38rpx;
}

.iconLogo-right {
  width: 75%;
  height: 212rpx;
  padding-right: 28rpx;
}

.logo-right-top {
  width: 100%;
  height: 152rpx;
  padding-top: 36rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
}

.logo-right-condition text {
  display: block;
  font-size: 40rpx;
  color: #191919;
  padding-top: 20rpx;
}

.roll-unused {
  margin-top: 16rpx;
  /* height: 150rpx; */
}

.roll-unused text {
  display: block;
}

.roll-unused text:last-child {
  color: #b1b1b1;
  font-size: 26rpx;
}

.roll-unused text:first-child {
  font-size: 28rpx;
  color: #555;
}

.roll-wrap-button {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  border-top: 1rpx solid #e0e0e0;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  color: #b1b1b1;
  font-size: 26rpx;
  align-items: center;
}
