<!--------------------------我的-------------------------------------------------------  -->
<!--My我的名片-->
<template name="userInfo">
  <view class="myEdit" bindtap="cardEdit">
    <view class="editTop">
      <view class="personName" wx:if="{{user.user_real_name}}">{{user.user_real_name}}</view>
      <view class="flex">
        <view class="personPosition" wx:if="{{user.user_company_career}}">{{user.user_company_career}}</view>
        <view class="personActive" wx:if="{{playTime==1 && user.active_status}}">{{user.active_status}}</view>
      </view>
      <view>
        <image wx:if="{{user.user_avatar_url}}" class="personPic" src="{{user.user_avatar_url}}"></image>
        <view wx:if="{{user.user_avatar_text}}" class="user_text noPic">{{user.user_avatar_text}}</view>
        <view wx:if="{{user.identify_status == 2 && playTime == 1}}">
          <image wx:if="{{user.authentication[0].group_id == 3}}" src="/img/img-rzchuangyezhe@2x.png" class='user_identifyContent'></image>
          <image wx:if="{{user.authentication[0].group_id == 18}}" src="/img/img-rzfa@2x.png" class='user_identifyContent'></image>
          <image wx:if="{{user.authentication[0].group_id == 21}}" src="/img/img-rzfa2@2x.png" class='user_identifyContent'></image>
          <image wx:if="{{user.authentication[0].group_id == 6}}" src="/img/img-rztouziren@2x.png" class='user_identifyContent'></image>
        </view>
      </view>
    </view>
    <view class="editBottom">
      <view class="personCompany flex">
        <image class='bottomPic' src='/img/icon-C@2x.png'></image>
        <view class="personCompanyContent">
          <label wx:if="{{user.user_brand}}" wx:if="{{user.user_brand}}">{{user.user_brand}} · </label>
          <label wx:if="{{user.user_company_name}}" wx:if="{{user.user_company_name}}">{{user.user_company_name}}</label>
        </view>
        <view class="checkNum" catchtap="searchIdentification" wx:if="{{IdentificationShow == 1&& playTime == 0}}">查税号</view>
      </view>
      <view class="personCompany flex">
        <image class='bottomPic' src='/img/icon-T@2x.png'></image>
        <view class="personTelephone" wx:if="{{user.user_mobile}}">
          <label data-telephone="{{user.user_mobile}}" id="{{blue==-1?'mobileStyle':''}}" catchtap="telephone">{{user.user_mobile}}</label>
        </view>
        <view wx:if="{{!user.user_mobile}}" class="personEmail">暂未透露</view>
      </view>
      <view class="personCompany flex">
        <image class='bottomPic' src='/img/icon-E@2x.png'></image>
        <view class="personEmail" wx:if="{{user.user_email}}">{{user.user_email}}</view>
        <view class="personEmail" wx:if="{{!user.user_email}}">暂未透露</view>
      </view>
    </view>
  </view>
</template>

<!--My个人描述-->
<template name="userIntro">
  <view class="personDes" bindtap="cardEdit" wx:if="{{user.user_intro!= '' || playTime == 1}}">
    <view class='flex shopBox'>
      <view class="personTitle">个人描述</view>
      <view class="allNum" wx:if="{{playTime == 1}}">编辑</view>
    </view>
    <view class='personDesContent' wx:if="{{user.user_intro!= ''}}" hidden="{{!user.user_intro}}">{{user.user_intro}}</view>
  </view>
</template>

<!--My寻找案源-->
<template name="InvestInfo">
  <view class="personDes" bindtap="findProjectEdit" wx:if="{{invest.invest_industry.length != 0 ||invest.invest_stage.length != 0 || invest.invest_scale.length != 0 ||invest.invest_desc != null|| playTime == 1}}">
    <view class='flex shopBox'>
      <view class="personTitle">投资需求</view>
      <view class="allNum" wx:if="{{playTime == 1}}">编辑</view>
    </view>
    <view class="requireBox" hidden="{{invest.invest_industry.length==0 || invest.length == 0}}">
      <view class="requireTitle">· 投资领域</view>
      <view class='flex industryBox'>
        <block wx:for="{{invest.invest_industry}}" wx:key="{{item.industry_id}}">
          <view class="allIndustry" data-id="{{item.industry_id}}" data-value="{{item.industry_name}}">{{item.industry_name}}</view>
        </block>
      </view>
    </view>
    <view class="requireBox" wx:if="{{invest.invest_stage!=''}}" hidden="{{invest.invest_stage.length == 0 || invest.length == 0}}">
      <view class="requireTitle">· 投资轮次</view>
      <view class='flex industryBox'>
        <block wx:for="{{invest.invest_stage}}" wx:key="{{item.stage_id}}">
          <view class="allIndustry" data-id="{{item.stage_id}}" data-value="{{item.stage_name}}">{{item.stage_name}}</view>
        </block>
      </view>
    </view>
    <view class="requireBox" wx:if="{{invest.invest_scale!=''}}" hidden="{{invest.invest_scale.length == 0 || invest.length == 0}}">
      <view class="requireTitle">· 投资金额</view>
      <view class='flex industryBox'>
        <block wx:for="{{invest.invest_scale}}" wx:key="{{item.scale_id}}">
          <view class="allIndustry" data-id="{{item.scale_id}}" data-value="{{item.scale_money}}">{{item.scale_money}}</view>
        </block>
      </view>
    </view>
    <view hidden="{{!invest.invest_desc}}">
      <view class="requireTitle">· 具体描述</view>
      <view class="requireDes">{{invest.invest_desc}}</view>
    </view>
  </view>
</template>

<!--My资源对接-->
<template name="resourceInfo">
  <view class="user_resource_info publish-resourcesm" bindtap="resourceEnchangeEdit">
    <view class="flex title">
      <view class="title">
        <image class="title_icon" src="/img/icon-duijie@3x.png"></image>
        资源需求
      </view>
      <view class="flex">
        <image class="iconEdit" src="/img/icon-edit@3x.png"></image>
      </view>
    </view>

    <!--可提供资源-->
    <view class="resource_title" wx:if="{{resource.res_give!=''}}" hidden="{{!resource.res_give}}">拥有的资源</view>
    <view class="res_give_frame" wx:if="{{resource.res_give!=''}}" hidden="{{!resource.res_give}}">
      <block wx:for="{{resource.res_give}}" wx:key="{{item.resource_id}}">
        <label class="investStyle" wx:if="{{index < resource.res_give.length-1}}" data-id="{{item.resource_id}}" data-value="{{item.resource_name}}">{{item.resource_name}}、</label>
        <label class="investStyle" wx:if="{{index == resource.res_give.length-1}}" data-id="{{item.resource_id}}" data-value="{{item.resource_name}}">{{item.resource_name}}</label>
      </block>
    </view>

    <!--正寻求资源-->
    <view class="resource_title" wx:if="{{resource.res_find !=''}}" hidden="{{!resource.res_find}}">寻求的资源</view>
    <view class="res_find_frame" wx:if="{{resource.res_find !=''}}" hidden="{{!resource.res_find}}">
      <block wx:for="{{resource.res_find}}" wx:key="{{item.resource_id}}">
        <label class="investStyle" wx:if="{{index < resource.res_find.length-1}}" data-id="{{item.resource_id}}" data-value="{{item.resource_name}}">{{item.resource_name}}、</label>
        <label class="investStyle" wx:if="{{index ==  resource.res_find.length-1}}" data-id="{{item.stage_id}}" data-value="{{item.stage_name}}">{{item.resource_name}}</label>
      </block>
    </view>
    <!--具体描述  -->
    <view class="invest_title" hidden="{{!resource.res_desc}}">具体描述</view>
    <view class="investStyle" hidden="{{!resource.res_desc}}">{{resource.res_desc}}</view>
  </view>
</template>

<!--项目店铺-->
<template name="financingProject">
  <view class='personDes' wx:if="{{count.project_count != 0 || playTime == 1}}">
    <view class='flex shopBox' catchtap="projectFinance">
      <view class="personTitle">项目店铺</view>
      <view class="allNum" wx:if="{{count.project_count > 3}}">全部({{count.project_count}})</view>
      <view class="allNum" wx:if="{{count.project_count <= 3 && playTime == 1}}">编辑</view>
    </view>
    <view class='outBox'>
      <block wx:for="{{project_info}}" wx:key="{{item.pro_intro}}">
        <view class='insideBox' class="user_project_frame" hover-class='t-hoverColor' catchtap="financingDetail" data-id="{{item.pro_id}}">
          <view class="describle" wx:if="{{item.pro_intro != ''}}">{{item.pro_intro||item.case_name}}</view>
          <image src="/img/img-dujiaqianyue@2x.png" class="projectDetailType" wx:if="{{item.is_exclusive == 1}}"></image>
          <view class='flex industryBox'>
            <block wx:for="{{item.pro_industry||item.case_industry}}" wx:key="{{item.industry_id}}" wx:for-item="pro_industry">
              <view class="allIndustry" data-id="{{pro_industry.industry_id}}" data-vlaue="{{pro_industry.industry_name}}">{{pro_industry.industry_name}}</view>
            </block>
          </view>
          <view class='flex otherInfo'>
            <view class="allProjectDetail flex" wx:if="{{item.pro_area != ''||item.pro_finance_stock_after!=0||item.pro_stage!=''||item.pro_scale!=''}}">
              <view class="projectDetailLine" wx:if="{{item.pro_scale.scale_money}}">{{item.pro_scale.scale_money}} </view>
              <view class="projectDetailLine" wx:if="{{item.pro_finance_stock_after}}">
                <label wx:if="{{item.pro_scale.scale_money}}">·</label> {{item.pro_finance_stock_after}}% </view>
              <view class="projectDetailLine" wx:if="{{item.pro_stage.stage_name}}">
                <label wx:if="{{item.pro_finance_stock_after}}"> · </label>{{item.pro_stage.stage_name}}</view>
              <view class="projectDetailLine" wx:if="{{item.pro_city.area_title}}">
                <label wx:if="{{item.pro_stage.stage_name}}">· </label>{{item.pro_city.area_title}}</view>
            </view>
          </view>
        </view>
      </block>
    </view>
  </view>
</template>

<!--成功案例-->
<template name="investProject">
  <view class="personDes" catchtap="investCase" wx:if="{{invest_case.length != 0 || playTime == 1}}" id="investProject">
    <view class='flex shopBox'>
      <view class="personTitle">成功案例</view>
      <view class="allNum" wx:if="{{invest_case.length > 3}}">全部({{invest_case.length}})</view>
      <view class="allNum" wx:if="{{invest_case.length<= 3 && playTime == 1}}">编辑</view>
    </view>
    <block wx:for="{{invest_case}}" wx:key="{{item.case_id}}">
      <view class="successProjcet flex" catchtap="investCase" data-id="{{item.case_id}}" data-index="{{index}}" hidden="{{index >= 3}}">
        <view class="successLeft">
          <view class="successLeftTime" wx:if="{{item.case_deal_time}}">{{item.case_deal_time}}</view>
          <view class="successLeftTurn" wx:if="{{item.stage.stage_name}}">{{item.stage.stage_name}}</view>
        </view>
        <view class="successRight">
          <view class='flex'>
            <image src='/img/img-dot@2x.png' class="successPic"></image>
            <view class="successProjcetName" wx:if="{{item.case_name}}">{{item.case_name}}</view>
          </view>
          <view>
            <view class="successRightArea">
              <label wx:if="{{item.case_money}}">{{item.case_money}}</label>
              ·
              <label wx:if="{{item.case_area}}">{{item.case_area.area_title}}</label>
            </view>
            <view class='flex industryBox'>
              <block wx:for="{{item.industry}}" wx:key="{{item.industry_id}}" wx:for-item="pro_industry">
                <view class="allIndustry" data-id="{{pro_industry.industry_id}}" data-vlaue="{{pro_industry.industry_name}}">{{pro_industry.industry_name}}</view>
              </block>
            </view>
          </view>
        </view>
      </view>
    </block>
  </view>

</template>

<!--项目融资列表 && 投资案例列表-->
<template name="projectList">
  <view class="weui-panel__bd" style="background:white;margin:16px" hover-class='t-hoverColor'>
    <view class="weui-media-box weui-media-box_text my_detail" data-id='{{myProject.project_id}}' data-index="{{index}}" data-id='{{myProject.case_id}}' bindtap='detail'>
      <view class="title" style="text-align:left;">{{myProject.pro_intro||myProject.case_name}}</view>
      <view class="sort flex">
        <block wx:for="{{myProject.pro_industry||myProject.industry}}" wx:for-item="sort" wx:key="*this">
          <view class="weui-media-box__desc" wx:if="{{index<4}}">
            {{sort.industry_name}}
          </view>
        </block>
      </view>
      <view class="flex img_frame">
        <view class="tips flex">
          <view style="width:1px"></view>
          <view class="weui-media-box__desc tip">{{myProject.pro_stage.stage_name||myProject.stage.stage_name}}</view>
          <view class="weui-media-box__desc tip">{{myProject.case_area.area_title||myProject.pro_area.area_title}}</view>
          <view class="weui-media-box__desc tip">{{myProject.pro_scale.scale_money||myProject.case_money}}万</view>
        </view>
        <!-- <image class="editIcon" data-index="{{index}}" data-id="{{myProject.pro_id}}" src="/img/icon-pen.png" bindtap="editDetail"></image> -->
      </view>
    </view>
  </view>
</template>

