#openHB .openKai {
  width: 176rpx;
  height: 176rpx;
  position: absolute;
  top: 535rpx;
  margin-left: -11%;
  font-size: 40rpx;
  color: #fff;
  z-index: 666;
  transition: All 0.4s ease-in-out;
  -webkit-transition: All 0.4s ease-in-out;
  -moz-transition: All 0.4s ease-in-out;
  -o-transition: All 0.4s ease-in-out;
}

#openHB .openKai1 {
  width: 176rpx;
  height: 176rpx;
  position: absolute;
  top: 535rpx;
  margin-left: -11%;
  font-size: 40rpx;
  color: #fff;
  z-index: 666;
  animation: mymove 1s infinite ease-in;
}

@keyframes mymove {
  0% {
    transform: rotateY(0deg);
  }

  50% {
    transform: rotateY(180deg);
  }

  100% {
    transform: rotateY(360deg);
  }
}

