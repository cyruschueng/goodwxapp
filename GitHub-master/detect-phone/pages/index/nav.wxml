<block wx:if="{{item.data.length > 0}}">
  <view class="fui-icon-group col-4 noborder circle">
    <block wx:for="{{item.data}}" wx:for-index="idx" wx:key="">
      <navigator url="{{item.url}}" class="fui-icon-col">
        <view>
          <image src="{{item.icon}}"></image>
        </view>
        <view class="navs-navname">{{item.navname}}</view>
      </navigator>
    </block>
  </view>
</block>