/* pages/before/before.wxss */
.wrap{
  width: 750rpx;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
}

.wrap image{
  width: 100%;
  height: 100%;
}

.second{
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  padding: 0 20rpx;
  height: 50rpx;
  line-height: 50rpx;
  background: rgba(0,0,0,0.5);
  color: #fff;
  border-radius: 25rpx;
  font-size: 30rpx;
}