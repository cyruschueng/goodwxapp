/*规则说明弹窗  */

.shade_bg {
  position: fixed;
  z-index: 500;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  background-color: rgba(0, 0, 0, 0.5);
}

.modal {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%) ;
  width: 630rpx;
  animation: ruleAni 300ms linear forwards;
  -webkit-animation: ruleAni 300ms linear forwards;
}
@keyframes ruleAni{
  0%{
    transform:translate(-50%, -50%) scale(0.3,0.3);
    -webkit-transform:translate(-50%, -50%) scale(0.3,0.3);
  }
  80%{
    transform:translate(-50%, -50%) scale(1.2,1.2);
    -webkit-transform:translate(-50%, -50%) scale(1.2,1.2);
  }
  100%{
    transform:translate(-50%, -50%) scale(1,1);
    -webkit-transform:translate(-50%, -50%) scale(1,1);
  }
}
.modTop {
  position: relative;
}

.modBgLf, .modBgRg {
  position: relative;
  display: inline-block;
  vertical-align: bottom;
  width: 265rpx;
  height: 50rpx;
  background-color: #fd5c22;
}

.modBgLf {
  border-top-left-radius: 16rpx;
}

.modBgLfInner {
  position: absolute;
  top: -40rpx;
  right: 0;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  background-color: #fd5c22;
}

.modBgRg {
  border-top-right-radius: 16rpx;
}

.modBgRgInner {
  position: absolute;
  top: -76rpx;
  left: 0;
  width: 80rpx;
  height: 80rpx;
  border-radius: 40rpx 40rpx 0 0;
  background-color: #fd5c22;
}

.modBgMid {
  display: inline-block;
  vertical-align: bottom;
  position: relative;
  width: 100rpx;
  height: 50rpx;
  overflow: hidden;
}

.modBgMidInner {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 200%;
  background: radial-gradient(transparent 69%, #fd5c22 69%);
}

.modTop .modTopWhite {
  position: absolute;
  top: 0;
  left: 0;
  transform: translate(-8rpx, -6rpx);
}

.modTopWhite .modBgLf, .modTopWhite  .modBgLfInner, .modTopWhite .modBgRg,
.modTopWhite .modBgRgInner {
  background-color: #fff;
}

.modTopWhite  .modBgMidInner {
  background: radial-gradient(transparent 69%, #fff 69%);
}

