.popupBox {
  position: fixed;
  z-index: 400;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
}

.getAwaModal {
  position: absolute;
  z-index: 600;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  width: 600rpx;
  height: 530rpx;
  padding: 40rpx 68rpx;
  background-color: #fff;
  border-radius: 12rpx;
  animation: popup 700ms linear forwards;
  -webkit-animation: popup 700ms linear forwards;
}

@keyframes popup {
  0% {
    transform: scale(1, 1);
    -webkit-transform: scale(1, 1);
  }

  50% {
    transform: scale(1.25, 1.25);
    -webkit-transform:scale(1, 1);
  }

  100% {
    transform: scale(1, 1);
    -webkit-transform:scale(1, 1);
  }
}

