.ljcyBtn {
  position: absolute;
  z-index: 200;
  bottom: -36rpx;
  left: 50%;
  transform: translate(-50%);
  width: 280rpx;
  height: 72rpx;
  line-height: 72rpx;
  color: #fef7f3;
  font-size: 30rpx;
  background: linear-gradient(45deg, #fb870d, #fd690e);
}

.ljcyBtn::before {
  content: '';
  position: absolute;
  left: 0;
  top: 0;
  transform: translateX(-100%);
  width: 30rpx;
  height: 100%;
  clip-path: polygon(0 36rpx, 30rpx 0, 30rpx 72rpx);
  background-color: #fb870d;
}

.ljcyBtn::after {
  content: '';
  position: absolute;
  left: 100%;
  top: 0;
  width: 30rpx;
  height: 100%;
  clip-path: polygon(30rpx 36rpx, 0 0, 0 72rpx);
  background-color: #fd690e;
}

.ljcyBtn:active {
  opacity: 0.9;
}

.overBox .ljcyBtn {
  display: none;
}

.overTip {
  position: absolute;
