.baibian {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  width: 120rpx;
  height: 120rpx;
  border-radius: 6rpx;
  background-color: #b2b2b2;
  border: 4rpx solid #939292;
  display: flex;
  justify-content: center;
  align-items: center;
  transform: rotate(45deg) scale(0.8);
}

.baibian view {
  width: 100%;
  height: 100%;
  position: relative;
  transform: rotate(-90deg);
}

.baibian::after, .baibian::before, .baibian view::after, .baibian view::before {
  content: "";
  position: absolute;
  left: 0;
  top: 0;
  border: 4rpx solid #939292;
  width: 12rpx;
  height: 12rpx;
  display: block;
  transform: translate(-50%, -50%);
}

.baibian::before, .baibian view::before {
  top: auto;
  left: auto;
  right: 0;
  bottom: 0;
  transform: translate(50%, 50%);
}

