/*抽中字  */

.luckwordPic {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  width: 750rpx;
  height: 750rpx;
  transform: scale(1.2);
  opacity: 0.6;
  display: block;
  animation: luckwordPic 2.5s 0.3s linear forwards;
}

@keyframes luckwordPic {
  0% {
    transform: scale(1.2);
  }

  40% {
    transform: scale(0.9);
  }

  60% {
    transform: scale(0.9);
  }

  90% {
    transform: scale(1.3);
  }

  95% {
    transform: scale(1.15);
  }

  100% {
    transform: scale(1.2);
  }
}

.luckword {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  background: #feac1d;
  box-shadow: 0 0 0 10rpx #d33d24 inset, 0 0 0 8rpx #fff889;
  color: #f82612;
  font-size: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  animation: luckword 2s 0.3s linear forwards;
  transform: perspective(0px) scale(0) rotateX(480deg);
}

@keyframes luckword {
  0% {
    transform: perspective(0px) rotateX(720deg) scale(0);
  }

  40% {
    transform: perspective(180px) rotateX(400deg) scale(2);
  }

  60% {
    transform: perspective(140px) rotateX(300deg) scale(2);
  }

  90% {
    transform: perspective(0px) rotateX(0deg) scale(0.9);
  }

  95% {
    transform: perspective(0px) rotateX(0deg) scale(1.05);
  }

  100% {
    transform: perspective(0px) rotateX(0deg) scale(1);
  }
}

.luckwordFont {
  opacity: 0;
  transform: scale(0);
  animation: luckwordFont 0.8s 2.5s ease forwards;
}

@keyframes luckwordFont {
  0% {
    transform: scale(0);
    opacity: 0;
  }

  33% {
    transform: scale(1.2);
    opacity: 1;
  }

  66% {
    transform: scale(0.9);
    opacity: 1;
  }

  100% {
    transform: scale(1);
    opacity: 1;
  }
}

