/* pages/me/index.wxss */

.user {
  position: relative;
  text-align: center;
  margin: 30rpx;
}

.face image {
  border-radius: 50%;
  width: 200rpx;
  height: 200rpx;
}

.user navigator {
  position: absolute;
  top: 0;
  right: 0;
  font-size: 30rpx;
  border: 1rpx solid #e0e0e0;
  padding: 5rpx 25rpx;
  background-color: #fff;
}
