/* pages/test2/test2.wxss */

page {
    width: 750rpx;
    height: 100%;
}

.common-css {
    padding: 0;
    margin: 0;
    font-family: 'Microsoft YaHei';
    box-sizing: border-box;
}

.common-css::after, .common-css::before {
    content: '';
    visibility: hidden;
}

.canvas {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 999;
}