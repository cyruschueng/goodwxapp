.contenter{width:750rpx;min-height:772rpx;background:#e1ba1f;position:relative;padding:116rpx 0 40rpx}
.bgimg{width:750rpx;height:772rpx;position:absolute;top:0;left:0;z-index:1}
.money{width:120rpx;line-height:80rpx;height:80rpx;overflow:hidden;text-align:center;font-size:60rpx;color:#ff0;font-weight:700;margin:120rpx 158rpx 270rpx;position:relative;z-index:2}
.get{width:480rpx;margin:0 auto 80rpx;position:relative;z-index:999}
.get button{background:#f33;color:#fff}
.notes{width:640rpx;background:#fff;margin:0 auto;border-radius:20rpx;border:6rpx solid #cba71d;padding:40rpx;color:#f66}
.note-title{margin-top:20rpx;border-top:2rpx solid #f66}
.note-title text{background:#fff;padding:0 20rpx;position:relative;top:-22rpx}
.note-content{padding:0 20rpx}
.note-list{display:block;margin:20rpx 0;border-bottom:2rpx dashed #ddd}