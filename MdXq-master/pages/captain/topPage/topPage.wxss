.topPageTop{
  width:100%;
  height: 300rpx; 
  position: relative;
}
.topPageTop image{
  width: 100%;
  height: 100%;
}
.topPageTop view{
  width: 50%;
  text-align: center;
  position: absolute;
  bottom: 30rpx;
  background-color: #F8F8F8;
  left: 25%;
  font-size: 30rpx;
  padding: 8rpx 0;
  color: #F9B336;
}
.myTopBox{
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx 30rpx 20rpx 70rpx;
  overflow: hidden;
  border-bottom: 3rpx solid #EEEEEE;
  border-top: 20rpx solid #F8F8F8;
  font-size: 30rpx;
}
.headerBox{
  float: left;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background-color: #EEEEEE;
}
.headerBox image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.infoBox{
  margin-top: 7rpx;
  float: left;
  margin-left: 30rpx;
}
.infoBox view:first-child{
  font-size: 34rpx;
}
.infoBox view:last-child{
  color: #cccccc;
  margin-top: 10rpx;
}
.achievementBox{
  margin-top: 30rpx;
  float: right;
  text-align: left;
  width: 36%;
  max-width: 45%;
}
.userNum{
  float: left;
  position: absolute;
  left: 30rpx;
  top: 35%;
}
.topBox{

}
.topBoxTitle{
  font-size: 34rpx;
  padding: 20rpx;
  border-bottom: 1rpx solid #D2D2D2;
}
.newMyTopBox{
  border-top: 0;
  border-bottom: 1rpx solid #D5D5D5;
  position: relative;
}