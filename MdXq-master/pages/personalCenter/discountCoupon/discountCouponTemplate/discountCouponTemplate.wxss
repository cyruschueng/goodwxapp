.discountCouponList{
  width: 100%;
  background-color: white;
  overflow: hidden;
  box-sizing: border-box;
   position: relative;
   margin-bottom: 20rpx;
}
.discountCouponListActive{
  border: 1rpx solid red;
}
.discountCouponListLeft{
  font-size: 46rpx;
  font-weight: 700;
  float: left;
  width: 30%;
  padding: 60rpx 30rpx;
  box-sizing: border-box;
  text-align: center;
  border-right: 1rpx solid #F8F8F8;
  margin-top: 25rpx;
  position: relative;
}
.discountCouponListRight{
  float: left;
  font-size: 30rpx;
  padding: 55rpx 30rpx;
}
.discountCouponListRight view:first-child{
  font-size: 34rpx;
  margin-bottom: 20rpx;
}
/* .discountCouponListRight view:last-child text{
  color: #BEBEBE;
} */
.circleBox,.newCircleBox{
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  background-color: #F8F8F8;
  display: inline-block;
  position: absolute;
  top: 40%;
}
.circleBox{
  left: 0;
}
.newCircleBox{
  right: -20rpx;
}
.reasonBox{
  font-size: 30rpx;
  color: #BEBEBE;
  float: left;
  padding-bottom: 25rpx;
  padding-left: 30rpx;
}
.reasonBox text:first-child{
  color: red;
}